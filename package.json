    "@storybook/react-vite": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@tailwindcss/cli": "^4.0.15",
    "@types/react": "^19.0.12",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
