{
  "name": "apex-lib-shadcn-tailwind",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rollup -c --environment FORMAT:cjs",
    "build:esm": "rollup -c --environment FORMAT:esm",
    "copy-styles": "cp dist/cjs/styles.css dist/ && cp dist/esm/styles.css dist/",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "check-format": "prettier --check ./src/**/*.{ts,tsx}",
    "tscheck": "tsc --noEmit",
    "tscheck:watch": "tsc --noEmit --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-css": "npx @tailwindcss/cli -i ./src/index.css -o ./src/output.css",
    "build-css-w": "npx @tailwindcss/cli -i ./src/index.css -o ./src/output.css --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "keywords": [],
  "author": "Stefanus Lumban Tobing",
  "license": "ISC",
